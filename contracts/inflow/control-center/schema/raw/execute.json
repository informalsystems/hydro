{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Submits the total amount of tokens deployed by the whitelisted addresses. Action can be performed only by the whitelisted addresses.",
      "type": "object",
      "required": [
        "submit_deployed_amount"
      ],
      "properties": {
        "submit_deployed_amount": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Updates the total amount of tokens deployed by adding or subtracting the specified amount. Action can be performed only by the associated sub-vault smart contracts.",
      "type": "object",
      "required": [
        "update_deployed_amount"
      ],
      "properties": {
        "update_deployed_amount": {
          "type": "object",
          "required": [
            "amount",
            "direction"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            },
            "direction": {
              "$ref": "#/definitions/DeploymentDirection"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Adds a new account address to the whitelist.",
      "type": "object",
      "required": [
        "add_to_whitelist"
      ],
      "properties": {
        "add_to_whitelist": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Removes an account address from the whitelist.",
      "type": "object",
      "required": [
        "remove_from_whitelist"
      ],
      "properties": {
        "remove_from_whitelist": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Updates the configuration of the Control Center contract.",
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "type": "object",
          "required": [
            "config"
          ],
          "properties": {
            "config": {
              "$ref": "#/definitions/UpdateConfigData"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Adds a new sub-vault smart contract to be managed by the Control Center.",
      "type": "object",
      "required": [
        "add_subvault"
      ],
      "properties": {
        "add_subvault": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Removes a sub-vault smart contract from being managed by the Control Center.",
      "type": "object",
      "required": [
        "remove_subvault"
      ],
      "properties": {
        "remove_subvault": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "DeploymentDirection": {
      "type": "string",
      "enum": [
        "add",
        "subtract"
      ]
    },
    "Uint128": {
      "description": "A thin wrapper around u128 that is using strings for JSON encoding/decoding, such that the full u128 range can be used for clients that convert JSON numbers to floats, like JavaScript and jq.\n\n# Examples\n\nUse `from` to create instances of this and `u128` to get the value out:\n\n``` # use cosmwasm_std::Uint128; let a = Uint128::from(123u128); assert_eq!(a.u128(), 123);\n\nlet b = Uint128::from(42u64); assert_eq!(b.u128(), 42);\n\nlet c = Uint128::from(70u32); assert_eq!(c.u128(), 70); ```",
      "type": "string"
    },
    "UpdateConfigData": {
      "type": "object",
      "properties": {
        "deposit_cap": {
          "anyOf": [
            {
              "$ref": "#/definitions/Uint128"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "additionalProperties": false
    }
  }
}
