        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "total_pool_value"
      ],
      "properties": {
        "total_pool_value": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
